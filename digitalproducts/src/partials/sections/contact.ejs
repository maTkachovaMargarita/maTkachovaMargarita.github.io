<section class="contact" id="contact">
	<div id="map"></div>
	<div class="container">
		<div class="row">
			<div class="col-10 offset-1">
				<div class="contact__wrapper-title">
					<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="42" height="60" viewBox="0 0 42 60" class="contact__svg">
						<image width="42" height="60" xlink:href="data:img/png;base64,"/>
					</svg>
					<h2 class="contact__title">get in touch</h2>
				</div>
				<form action="#" class="contact__form">
					<div class="contact__wrapper-inform">
						<input type="text" class="contact__name" placeholder="Your Name*" required>
						<input type="email" class="contact__e-mail" placeholder="Your Email*" required>
					</div>
					<textarea class="contact__message" name="message" cols="30" rows="10"  placeholder="Your Message*" required></textarea>
					<input type="submit" class="contact__btn" value="send Message">
				</form>
			</div>
		</div>
	</div>
	<div class="container-fluid pr-0">
		<div class="contact__for-contact">
			<div class="contact__wrapper">
				<div class="contact__home">
					<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="38" height="36" viewBox="0 0 38 36">
						<image id="Icon-Address" width="38" height="36" xlink:href="data:img/png;base64,"/>
					</svg>
				</div>
				<p class="contact__adres">
					Ikan Piranha Atas 220C Malang - East Java Indonesia
				</p>
			</div>
			<div class="contact__wrapper">
				<div class="contact__tel">
					<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20" height="34" viewBox="0 0 20 34">
						<image width="20" height="34" xlink:href="data:img/png;base64,"/>
					</svg>
				</div>
				<p class="contact__phone">+13051489657, 
				+17256838256</p>
			</div>
		</div>
	</div>
</section>